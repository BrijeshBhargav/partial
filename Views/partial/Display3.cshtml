@using MVCWebAppHTD3.Models
@model dynamic

@{
    ViewData["Title"] = "Display3";
    Layout = "~/Views/Shared/_userLayout.cshtml";
}


<head>
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css" rel="stylesheet">
   
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
    
    
</head>


    <h1>Address Details</h1>
    <div>
<a asp-action="insert" class=" btn btn-sm btn-info">
        <i class="fa fa-plus"></i> <b>ADD Address</b>
    </a>
</div>
  <table class="table table-success">
   <thead class="table-dark">
        <tr>
            <th>Name</th>
            <th>City </th>
            <th>State</th>
           
        </tr>
        </thead>

        @foreach (AddressModel obj in Model.Address)
        {
            <tr>
                <td>@obj.Name</td>
                <td>@obj.City</td>
                <td>@obj.State</td>

            </tr>
        }
        <tr>

    </table>
    </div>
   
    <hr />
<h1>Family Details</h1>
<div>
    <a asp-action="insert2" class=" btn btn-sm btn-info">
        <i class="fa fa-plus"></i><b>ADD Family</b>
    </a>
</div>
    <table class="table table-success">
        <thead class="table-dark">
        <tr>
            <th>DoorNo</th>
            <th>Fathername</th>
            <th>Mother Name </th>
        </tr>
        </thead>
        @foreach (FamilyModel employee in Model.Family)
        {
            <tr>
                <td>@employee.DoorNo</td>
                <td>@employee.FatherName</td>
                <td>@employee.MotherName</td>
            </tr>
        }
    </table>

</asp:Scripmanager>


